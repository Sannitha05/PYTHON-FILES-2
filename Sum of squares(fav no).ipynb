    "    visited = set()\n",
    "    while n != 1 and n not in visited:\n",
    "        visited.add(n)\n",
    "        arr =sum(int(digit**2 for digit in str(n)))\n",
    "        return \"Yes\" if n==1 else \"No\"\n",
    "\n",
    "num = int(input())\n",
    "print(fav_no(num))"
